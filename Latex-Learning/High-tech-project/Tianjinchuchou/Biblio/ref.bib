
@article{zhuanghaifeng_2016,
	title = {缺氧/好氧移动床生物膜反应器短程脱氮工艺深度处理煤化工废水性能},
	volume = {67},
	issn = {0438-1157},
	abstract = {煤化工废水生化处理出水仍含有大量有毒和难降解污染物,对环境具有严重的危害,采用缺/好氧移动床生物膜反应器(ANMBBR-MBBR)复合生物短程脱氮技术对煤化工废水进行深度处理。试验结果表明,生物组合工艺有效缓解了废水有毒抑制物和低碳氮比对生物脱氮工艺的负面作用,最佳运行条件为水力停留时间12 h,硝态氮/亚硝态氮混合液回流比200\%,该工艺对COD、氨氮和总氮的去除率分别为68.1\%、84.0\%和74.7\%,相应的出水浓度分别为48.0、4.8和13.9 mg·L{\textasciitilde}(-1),均达到了国家城镇污水处理厂污染物排放一级A标准;高有毒负荷下,与传统的A{\textasciitilde}2O生物脱氮工艺相比,该组合工艺具有更加稳定和高效的脱氮效能;而且ANMBBR有效地提高了废水生物降解性(BOD\_5/COD值增加至0.3),有利于短程硝化的高效运行,MBBR处理后出水有毒抑制物的数量和种类分别减少了84.4\%和54.5\%。因此,该组合工艺具有性能高效稳定和经济节约的技术优势,适于煤化工废水深度处理的工程化应用。},
	language = {中文;},
	number = {09},
	urldate = {2019-05-22},
	journal = {化工学报},
	author = {庄, 海峰 and 韩, 洪军 and 单, 胜道 and 薛, 向东},
	year = {2016},
	keywords = {biotechnology, moving bed, short-cut biological nitrogen removal, toxic inhibitors, wastewater, 废水, 有毒抑制物, 生物技术, 生物短程脱氮, 移动床},
	pages = {3919--3926}
}

