	number = {01 vo 11},
	journal = {环境工程学报},
	author = {李阳;王芬;于雷;季民;},
	year = {2017},
	keywords = {Fenton reaction, hydrogen peroxide, OR-SON, phenol, 双氧水, 芬顿反应, 苯酚},
	pages = {267--272}
}

